@model List<YouBikeProject.Models.YouBikeLogModel>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<table class="table table-striped">
    <thead class="thead-dark">
        <tr>
            <th scope="col">#</th>
            <th scope="col">LOGID</th>
            <th scope="col">SNO</th>
            <th scope="col">SBI</th>
            <th scope="col">BEMP</th>
            <th scope="col">MDAY</th>
            <th scope="col">UPDATEDATETIME</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <th scope="row">@(i + 1)</th>
                <td>@Model[i].LOGID</td>
                <td>@Model[i].SNO</td>
                <td>@Model[i].SBI</td>
                <td>@Model[i].BEMP</td>
                <td>@Model[i].MDAY</td>
                <td>@Model[i].UPDATEDATETIME</td>
            </tr>
        }

    </tbody>
</table>